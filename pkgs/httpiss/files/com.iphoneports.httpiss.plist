<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

	<key>Label</key>
	<string>com.iphoneports.httpiss</string>

	<key>ProgramArguments</key>
	<array>
		<string>/var/usr/sbin/squid</string>
		<string>--foreground</string>
		<string>-f</string>
		<string>/var/usr/share/httpiss/squid.conf</string>
	</array>

	<key>KeepAlive</key>
	<dict>
		<key>NetworkState</key>
		<true/>
	</dict>

</dict>
</plist>
